# Alias definitions.
# These aliases are refered by ~/.bashrc
# See /usr/share/doc/bash-doc/examples in the bash-doc package.

# shell
alias h='history'
alias cls='clear'

# tmux 256 color support
alias tmux='TERM=xterm-256color tmux'

# python
alias py='python'
alias jy='jython'
alias ipy='ipython'

# system control
alias psmake="ps aux | grep 'make'"

